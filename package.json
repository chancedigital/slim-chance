{
  "name": "slim-chance",
  "version": "2.1.1",
  "description": "Custom WordPress theme for the Slim + Huskys website.",
  "homepage": "https://slimandhuskys.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/chancedigital/slim-chance"
  },
  "author": {
    "name": "Chance Strickland",
    "email": "hi@chancedigital.io",
    "url": "https://chancedigital.io",
    "role": "developer"
  },
  "scripts": {
    "setup": "yarn && composer install && composer install-codestandards",
    "dev": "NODE_ENV=development gulp dev --gulpfile build/gulpfile.babel.js",
    "start": "yarn dev",
    "watch": "NODE_ENV=development gulp --gulpfile build/gulpfile.babel.js watch",
    "build": "NODE_ENV=production gulp --gulpfile build/gulpfile.babel.js",
    "build:staging": "NODE_ENV=staging gulp --gulpfile build/gulpfile.babel.js",
    "lint-js": "eslint assets/js",
    "format-js": "eslint --fix assets/js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "@chancedigital/eslint-config/react",
      "@chancedigital/eslint-config/wp"
    ],
    "globals": {
      "jQuery": false,
      "wp": false
    },
    "rules": {
      "no-console": [
        1,
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "no-debugger": 1
    }
  },
  "eslintIgnore": [
    "assets/js/_vendor",
    "assets/js/admin/_vendor",
    "assets/js/frontend/_vendor",
    "assets/js/shared/_vendor"
  ],
  "stylelint": {
    "extends": [
      "@chancedigital/stylelint-config/wp"
    ],
    "ignore": [
      "**/*.js",
      "dist/**/*"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@chancedigital/eslint-config": "^2.0.1",
    "@chancedigital/stylelint-config": "^2.0.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-lodash": "^3.3.4",
    "browserslist": "^4.6.6",
    "caniuse-db": "^1.0.*",
    "dateformat": "^3.0.3",
    "del": "^5.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-wpcalypso": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-cache": "^1.1.2",
    "gulp-cssnano": "^2.1.3",
    "gulp-env": "^0.4.0",
    "gulp-filter": "^6.0.0",
    "gulp-header-comment": "^0.6.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^6.0.0",
    "gulp-livereload": "^4.0.1",
    "gulp-notify": "^3.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "git+https://github.com/chancestrickland/gulp-sass",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-string-replace": "^1.1.2",
    "gulp-zip": "^5.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "node-sass": "^4.12.0",
    "node-sass-tilde-importer": "^1.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "git+https://github.com/Automattic/wp-prettier#wp-prettier-1.17.0",
    "pump": "^3.0.0",
    "require-dir": "^1.2.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.9.1",
    "terser-webpack-plugin": "^1.3.0",
    "vinyl": "^2.2.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1",
    "webpack-stream": "^5.2.1",
    "webpackbar": "^3.2.0"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "classnames": "^2.2.6",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.14",
    "motion-ui": "^2.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-transition-group": "^4.2.1",
    "slick-carousel": "^1.8.1",
    "what-input": "^5.2.3"
  },
  "browserslist": [
    "> 2%",
    "not ie > 0"
  ],
  "engineStrict": true,
  "engines": {
    "node": "~10"
  }
}
