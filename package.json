    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.5.2",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "motion-ui": "^2.0.3",
    "slick-carousel": "^1.8.1",
    "what-input": "^5.1.3"
  },
  "browserslist": [
    "> 2%",
    "not ie > 0"
  ],
  "engineStrict": true,
  "engines": {
    "node": "~8.12"
  }
